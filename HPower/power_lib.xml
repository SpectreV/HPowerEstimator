<!-- k = 6 -->
<!-- N = 10 -->
<!-- I = 40 -->
<!-- Fi = 6 -->
<!-- frac_level = 0 -->
<!-- ff = 1 -->
<power_lib>
  	<pb_power_lib>
    	<pb_power name="io" power_method="ignore"/>			
		<pb_power name="clb" power_method="sum">
  			<pb_power name="ble" power_method="sum">
    			<pb_power name="soft_logic" power_method="sum">
      				<pb_power name="lut6" power_method="model" num_input="6" num_output="1">
          				<dynamic_power at_freq="2e8">
            					5.72e-7 3.58e-7 1.43e-7 8.64e-8 3.03e-8 1.40e-8
          				</dynamic_power>
      					<static_power>
            					1.54e-7
          				</static_power>
      				</pb_power>
			</pb_power>
    			<pb_power name="ff" power_method="macro" num_input="1" num_output="1">
  				<dynamic_power at_freq="2e8">
    	  				7.4e-7
				</dynamic_power>
				<static_power>
    	  				8e-9
				</static_power>
    			</pb_power>
  			</pb_power>
		</pb_power>
		<pb_power name="mult_36" power_method="sum">
  			<pb_power name="divisible_mult_18x18" power_method="sum">
      			<pb_power name="mult_9x9_slice" power_method="ignore"/>
      			<pb_power name="mult_18x18_slice" power_method="ignore"/>
      			<pb_power name="mult_36x36_slice" power_method="ignore"/>
  			</pb_power>
		</pb_power>
		<pb_power name="memory" power_method="ignore"/>
	</pb_power_lib>
	
	<route_power_lib switch_type="mux" power_method="sum">
		<c_block/>
		<s_block/>
	</route_power_lib>

  	<general_power_lib Vdd="1.05">
		<route_segment length="4" C_wire="1.11e-14"/>
    	<mux_power at_freq="2e8" size="4" dynamic="4.96e-8" static="4.90e-9"/>
		<buf_power at_freq="2e8" dynamic="2.64e-7" static="7.13e-9"/>
    	<mem_power static="2.41e-9" write_energy="2.02e-14"/>
  	</general_power_lib>
  	
  	

</power_lib>
